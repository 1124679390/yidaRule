lmnopqrstuvwxyz0123456789\", n = t.length, o = 0; 10 > o; o++)\n            a += t.charAt(Math.floor(Math.random() * n));\n        return `${a}?token=${token}&expiry=${Date.now()}`;\n    }\n    // print(`url:${url}`);\n    let gethost = url.match(/http.*?\\.\\w+\\//g)?.join() ?? '';\n    gethost = gethost.replace(/,.*/g, '');\n    gethost = gethost.substring(0, gethost.length - 1);\n    // print(`gethost:${gethost}`);\n    resp = await tools.httpRequest({ url: url, h2: true, headers: { 'referer': url } });\n    let link = resp.body.match(/\\/pass_md5.*?',/g)?.join() ?? '';\n\n    link = link.substring(0, link.length - 2);\n    let res = await tools.httpRequest({ url: `${gethost}${link}`, h2: true, headers: { 'referer': url } });\n    // print(res.body);\n    // print(`pass_md5_link:${link}`);\n    let paramsList = link.match(/\\/[\\w-]+/g);\n    // print(`paramsList:${paramsList}`);\n    let hash = paramsList[1] ?? '';\n    let token = paramsList[2] ?? '';\n    // print(`token:${token}`);\n    // print(`hash:${hash}`);\n    let startPlayUrl = `${gethost}/dood?op=watch&hash=${hash.substring(1)}&token=${token.substring(1)}&embed=1&ref2=${encodeURIComponent(url)}&adb=1&ftor=0`;\n    await tools.httpRequest({ url: startPlayUrl, h2: true, headers: { 'referer': url } });\n    // print(`startPlayUrl:${startPlayUrl}`);\n    link = res.body + makePlay(token.substring(1));\n    // print(`link:${link}`);\n    let headers = {};\n    headers['Referer'] = gethost + '/';\n    headers['Accept-Encoding'] = 'Accept-Encoding: identity;q=1, *;q=0';\n    headers['Accept-Language'] = 'Accept-Language: zh-CN,zh;q=0.9,eo;q=0.8,en;q=0.7,ko;q=0.6';\n    headers['User-Agent'] = 'Mozilla/5.0 (iPhone; CPU iPhone OS 16_4_1 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Mobile/15E148';\n    return `${link},${JSON.stringify(headers)}`;\n}\nresp = await tools.httpRequest({ \"url\": url, webview: true, sourceRegex: '(?:\\.m3u8|\\.mp4|api\\/source)' });\nlet parseUrl = resp.body;\nif (parseUrl.indexOf(\"/api/source\") != -1) {\n    videoUrl = JSON.parse(await http.post(url.replace(\"/v/\", \"/api/source/\"), \"r=&d=\")).data[0].file;;\n} else {\n    let headers = {};\n    headers['Connection'] = 'keep-alive';\n    headers['User-Agent'] = 'Mozilla/5.0 (iPhone; CPU iPhone OS 16_4_1 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Mobile/15E148';\n    if (url.indexOf(\"streamsb\") != -1) {\n        headers['Referer'] = 'https://streamsb.net/';\n        headers['Origin'] = 'https://streamsb.net';\n    }\n    if (url.indexOf(\"javplayer.me\") != -1) {\n        headers['Referer'] = 'https://javplayer.me/';\n        headers['Origin'] = 'https://javplayer.me';\n    }\n    videoUrl = `${parseUrl},${JSON.stringify(headers)}`;\n}\nreturn videoUrl;"},"discoverRules":[{"name":"全部","index":0,"searchRule":{"requestUrl":"@js:\nparams.tabIndex = 0;\nlet url =  '/category/all/page/' + params.pageIndex + '/';\n    return {\n        'url' : url, \n'h2':true\n    };","list":"//div[@class=\"col-md-4 col-sm-6\"]/div[1]","title":"{{h4@text}} {{h5@text}}","author":"//*[@class=\"btn btn-danger btn-xs m-r-5 m-t-2\"]/text()","cover":"//img/@data-src@js:return result.startsWith(\"//\") == true ? \"https:\" + result : result;","tags":"//*[@class=\"btn btn-inverse btn-xs m-r-5 m-t-2\"]/text()|//*[@class=\"btn btn-warning btn-xs m-r-5 m-t-2\"]/text()","detailUrl":"//*[@class=\"card-block\"]/a[1]/@href","moreKeys":"{\"requestFilters\":[]}","displayType":1}},{"name":"排行","index":1,"searchRule":{"requestUrl":"@js:\nparams.tabIndex = 1;\nlet host=config.host;\n              let url = '';\n    if (params.tabIndex == 0) {\n        url = host + '/category/all/page/' + params.pageIndex + '/';\n    }\n    else if (params.tabIndex == 1) {\n        url = host + params.filters.rank + '/page/' + params.pageIndex;\n    }\n    else if (params.tabIndex == 2) {\n        url = host + params.filters.cat + '/page/' + params.pageIndex;\n    }\n    else if (params.tabIndex == 3) {\n        url = ho